!!! 5
%html{lang: 'en'}
  %head
    %meta{charset: 'utf-8'}
    %title Photoshop Lite
    %link{rel: 'stylesheet', href: 'css/normalize.css'}
    %link{rel: 'stylesheet', href: 'css/style.css'}
  %body
    %section
      %h1 Photoshop Lite
      %input#color{type: 'text', autofocus: true, placeholder: '#ffcc33'}
      %input#add_color{type: 'button', value: 'Add Color!'}
      %span#brush
      .clear
      #colors
      .clear
      %input#amount{type: 'text', placeholder: '3'}
      %input#add_box{type: 'button', value: 'Add Boxes!'}
      .clear
      #boxes
      .clear

    %script{src: 'js/jquery-1.10.2.min.js'}
    %script{src: 'js/underscore-min.js'}
    %script{src: 'js/main.js'}
